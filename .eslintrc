{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "@typescript-eslint/ban-ts-comment": 0,
    "no-console": 2
  },
  "settings": {
    "react": {
      "version": "17.0"
    }
  }
}
