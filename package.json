{
  "name": "ng-login-ui",
  "version": "0.24.1",
  "description": "NextGen SignIn/SignUp UI Micro-frontend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "dev": " NODE_ENV=development  vite",
    "pretty": "prettier --write src",
    "build-main": "vite build",
    "build-onprem": " ON_PREM=true yarn build",
    "build": "yarn build-main",
    "serve": "vite preview",
    "services-portal": "restful-react import --config restful-react.config.js portal",
    "services-ng": "restful-react import --config restful-react.config.js ng",
    "services": "yarn services-portal && yarn services-ng",
    "postservices-portal": "prettier --write src/services/portal/index.tsx",
    "postservices-ng": "prettier --write src/services/ng/index.tsx",
    "typecheck": "tsc",
    "docker": "sh -c 'docker build -t us.gcr.io/platform-205701/harness/ng-auth-ui:\"$0\" . && docker push us.gcr.io/platform-205701/harness/ng-auth-ui:\"$0\"'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wings-software/ng-login-ui.git"
  },
  "keywords": [
    "ui",
    "login",
    "signup"
  ],
  "author": "Harness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wings-software/ng-login-ui/issues"
  },
  "homepage": "https://github.com/wings-software/ng-login-ui#readme",
  "dependencies": {
    "@wings-software/telemetry": "^1.0.36",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "purecss": "^2.0.5",
    "qs": "6.9.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-google-recaptcha": "^2.1.0",
    "react-hot-toast": "^1.0.2",
    "react-router-dom": "^5.2.0",
    "restful-react": "^15.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@testing-library/react": "^11.2.6",
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.12.2",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "final-form": "^4.20.2",
    "history": "4.10.1",
    "husky": "4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "path-to-regexp": "^6.2.0",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.2.1",
    "rollup-plugin-emit-ejs": "^3.1.0",
    "rollup-plugin-visualizer": "^4.2.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.3",
    "vite": "^2.1.0",
    "vite-plugin-html-config": "^1.0.5"
  },
  "postcss": {
    "plugins": {
      "postcss-nesting": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "prettier --check",
      "sh scripts/typecheck-staged.sh"
    ]
  },
  "resolutions": {
    "qs": "6.9.4"
  }
}
